@model List<Category>

<h1>Category</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Category ID
			</th>
			<th>
				Category Name
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model.OrderBy(u=>u.Id))
		{
			<tr>
				<td>
					@obj.Id
				</td>
				<td>
					@obj.Name
				</td>
			</tr>
		}
	</tbody>
</table>